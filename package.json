{
  "name": "smykbsl",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "ajv": "^6.5.2",
    "autoprefixer": "8.6.5",
    "babel-core": "6.26.3",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "blanket": "^1.2.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "codacy-coverage": "^3.4.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "flow-bin": "^0.76.0",
    "fs-extra": "6.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "prettier": "^1.13.7",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "resolve": "1.8.1",
    "semantic-ui-react": "^0.81.3",
    "style-loader": "0.21.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "whatwg-fetch": "2.0.4",
    "snyk": "^1.89.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!<rootDir>/src/semantic/**",
      "!<rootDir>/config/**",
      "!<rootDir>/build/**",
      "!<rootDir>/scripts/**"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/test/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "coverageDirectory": "/test-results/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.node/",
      "/jest/"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build-css build-js",
    "test": "node scripts/test.js --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "coverage": "npm test -- --coverage",
    "testCI": "yarn test --ci --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "mocha": "NODE_ENV=test mocha ./test/*.js --compilers js:babel-core/register --reporter spec",
    "test-with-coverage": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --compilers js:babel-core/register --require blanket --reporter mocha-lcov-reporter | codacy-coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.4.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.10.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "jest": "23.4.0",
    "jest-cli": "^23.4.0",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "sinon": "^6.1.3",
    "webpack": "4.16.0",
    "webpack-dev-server": "3.1.4",
    "webpack-manifest-plugin": "2.0.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "snyk": true
}
